{
  "name": "@aqua-stark/backend",
  "version": "1.0.0",
  "description": "Aqua Stark Backend API",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "lint": "eslint src/",
