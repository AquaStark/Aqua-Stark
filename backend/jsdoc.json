{
  "source": {
    "include": ["src/controllers/"],
    "includePattern": "\\.js$",
    "exclude": ["node_modules/"]
  },
  "opts": {
    "destination": "docs/controllers/",
    "recurse": true,
    "readme": "README.md",
    "template": "node_modules/jsdoc/templates/default"
  },
  "plugins": ["plugins/markdown"],
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true,
    "default": {
      "outputSourceFiles": true
    },
    "applicationName": "Aqua-Stark Backend Controllers",
    "footer": "Generated by JSDoc",
    "copyright": "Aqua-Stark Team Â© 2024",
    "navType": "vertical",
    "theme": "default",
    "linenums": true,
    "collapseSymbols": false,
    "inverseNav": true
  },
  "markdown": {
    "hardwrap": false,
    "idInHeadings": true
  }
}
