{
  "buildCommand": "npm install && npm run build",
  "outputDirectory": "client/dist",
  "installCommand": null,
  "rootDirectory": "client",
  "framework": null,
  "rewrites": [
    {
      "source": "/api/v1/(.*)",
      "destination": "/api/index.js"
    },
    {
      "source": "/api/(.*)",
      "destination": "/api/index.js"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
