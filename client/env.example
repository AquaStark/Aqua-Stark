# Aqua Stark Frontend Environment Variables
# Copy this file to .env.local and configure your settings

# Backend API Configuration
# For local development: http://localhost:3001
# For production: https://your-backend-url.vercel.app
VITE_API_URL=http://localhost:3001

# Feature Flags (true/false)
VITE_BACKEND_INTEGRATION=true
VITE_PLAYER_VALIDATION=true
VITE_REALTIME_UPDATES=true

# API Timeout (milliseconds)
VITE_API_TIMEOUT=10000

# Development Settings
VITE_DEBUG=false
