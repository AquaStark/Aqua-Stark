{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "env": {
    "VITE_BACKEND_INTEGRATION": "true",
    "VITE_PLAYER_VALIDATION": "true",
    "VITE_REALTIME_UPDATES": "true"
  }
}
